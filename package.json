{
  "name": "react-apollo-universal",
  "version": "1.0.0-alpha.0",
  "description": "React Apollo Universal starter backed by Express",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-apollo-universal"
  },
  "keywords": [
    "react",
    "apollo",
    "pwa",
    "ssr"
  ],
  "dependencies": {
    "@babel/plugin-proposal-decorators": "7.0.0",
    "apollo-client": "2.4.1",
    "apollo-server": "2.0.5",
    "compression": "1.7.3",
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "graphql": "0.13.2",
    "graphql-tools": "3.1.1",
    "history": "4.7.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.4.0",
    "redux": "4.0.0",
    "styled-components": "3.4.5"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.1",
    "babel-preset-setsun": "2.1.3",
    "concurrently": "4.0.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "eslint": "5.5.0",
    "eslint-config-eslint": "5.0.1",
    "eslint-config-prettier": "3.0.1",
    "eslint-config-setsun": "2.1.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "nodemon": "1.18.4",
    "prettier": "1.14.2",
    "style-loader": "0.23.0",
    "ts-loader": "4.5.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.1"
  },
  "scripts": {
    "watch:client": "cross-env NODE_ENV=development webpack --config webpack.config.client.js --watch",
    "watch:server": "cross-env NODE_ENV=development npx babel-node src/server",
    "watch": "concurrently --kill-others \"npm run watch:client\" \"npm run watch:server\"",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "build": "concurrently --kill-others \"npm run build:client\" \"npm run build:server\"",
    "start:dev": "cross-env NODE_ENV=development nodemon dist/server.js --watch dist/server.js",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "precommit": "lint-staged",
    "eslint-prettier-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
