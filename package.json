{
  "name": "react-relay-universal",
  "version": "0.2.0",
  "description": "React Relay Universal starter backed by Express",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-relay-universal"
  },
  "keywords": [
    "react",
    "relay",
    "pwa",
    "ssr"
  ],
  "dependencies": {
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.3",
    "graphql-relay": "^0.5.2",
    "history": "^4.6.1",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-relay": "^1.0.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^2.2.0",
    "redux": "^3.6.0",
    "styled-components": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-relay": "^1.0.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.26.0",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "0.28.7",
    "cross-env": "^5.0.5",
    "dotenv": "4.0.0",
    "eslint": "4.7.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.3.0",
    "file-loader": "0.11.2",
    "flow-bin": "^0.54.1",
    "nodemon": "^1.11.0",
    "relay-compiler": "^1.3.0",
    "source-map-support": "^0.4.15",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js --watch",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.config.client.js --watch",
    "dev:start": "cross-env NODE_ENV=development nodemon dist/server.js --watch dist/server.js",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.server.js && NODE_ENV=production webpack --config webpack.config.client.js",
    "relay": "relay-compiler --src ./src --schema path/schema.graphql",
    "flow": "flow"
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "relay"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
