{
  "name": "react-relay-universal",
  "version": "0.2.0",
  "description": "React Relay Universal starter backed by Express",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-relay-universal"
  },
  "keywords": ["react", "relay", "pwa", "ssr"],
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-relay": "^0.5.3",
    "graphql-tools": "^2.5.1",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-relay": "^1.4.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "relay-runtime": "^1.4.1",
    "styled-components": "2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-relay": "^1.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "compression-webpack-plugin": "^1.0.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.9.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "1.1.5",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "relay-compiler": "^1.4.1",
    "source-map-support": "^0.5.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "watch:client":
      "cross-env NODE_ENV=development webpack --config webpack.config.client.js --watch",
    "watch:server":
      "cross-env NODE_ENV=development webpack --config webpack.config.server.js --watch",
    "watch":
      "concurrently --kill-others \"npm run watch:client\" \"npm run watch:server\"",
    "build:client":
      "cross-env NODE_ENV=production webpack --config webpack.config.client.js",
    "build:server":
      "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "build":
      "concurrently --kill-others \"npm run build:client\" \"npm run build:server\"",
    "start:dev":
      "cross-env NODE_ENV=development nodemon dist/server.js --watch dist/server.js",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "precommit": "lint-staged",
    "relay": "relay-compiler --src ./src --schema path/schema.graphql",
    "eslint-prettier-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  },
  "babel": {
    "presets": [
      "react",
      "stage-0",
      "es2015",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": ["relay"]
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "eslintConfig": {
    "extends": ["eslint", "prettier"],
    "plugins": ["import", "react", "prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "printWidth": 80
        }
      ]
    }
  }
}
