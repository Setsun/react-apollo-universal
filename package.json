{
  "name": "react-apollo-universal",
  "version": "1.0.0-alpha.0",
  "description": "React Apollo Universal starter backed by Express",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-apollo-universal"
  },
  "keywords": [
    "react",
    "apollo",
    "pwa",
    "ssr"
  ],
  "dependencies": {
    "apollo-cache-inmemory": "1.2.10",
    "apollo-client": "2.4.2",
    "apollo-link": "1.2.3",
    "apollo-server": "2.1.0",
    "apollo-server-express": "2.1.0",
    "express": "4.16.3",
    "graphql": "14.0.2",
    "graphql-tag": "2.9.2",
    "graphql-tools": "4.0.0",
    "history": "4.7.2",
    "http-proxy-middleware": "0.19.0",
    "react": "16.5.2",
    "react-apollo": "2.1.11",
    "react-dom": "16.5.2",
    "react-intl": "2.6.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.4.0",
    "styled-components": "^4.0.0-beta.8",
    "zen-observable": "0.8.9"
  },
  "devDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/node": "7.0.0",
    "@types/express": "4.16.0",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.2",
    "babel-preset-setsun": "2.1.3",
    "concurrently": "4.0.1",
    "cross-env": "5.2.0",
    "dotenv": "6.0.0",
    "eslint": "5.6.0",
    "eslint-config-eslint": "5.0.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-setsun": "2.1.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.0.0-rc.15",
    "lint-staged": "7.3.0",
    "nodemon": "1.18.4",
    "prettier": "1.14.3",
    "ts-loader": "5.2.0",
    "typescript": "3.0.3",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.1",
    "webpack-serve": "2.0.2",
    "workbox-webpack-plugin": "3.6.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cross-env NODE_ENV=development webpack-serve --config webpack.config.js",
    "start:server": "cross-env NODE_ENV=development nodemon src/server --exec babel-node --extensions '.ts,.tsx'",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "cross-env NODE_ENV=production babel src/server.tsx --out-file dist/server.js",
    "serve": "node dist/server.js",
    "type-check": "tsc",
    "eslint-prettier-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}