{
  "name": "react-apollo-universal",
  "version": "1.0.0-rc.1",
  "description": "React Apollo Universal starter backed by Express",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-apollo-universal"
  },
  "keywords": [
    "react",
    "apollo",
    "pwa",
    "ssr"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "start:server": "cross-env NODE_ENV=development nodemon src/server --exec babel-node --extensions '.ts,.tsx'",
    "build": "concurrently --kill-others \"npm run build:client\" \"npm run build:server\"",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "cross-env NODE_ENV=production babel src/server.tsx --out-file dist/server.js"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-server": "2.5.0",
    "apollo-server-express": "2.5.0",
    "express": "^5.0.0-alpha.7",
    "graphql": "^14.3.0",
    "passport": "^0.4.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "stripe": "^7.0.0",
    "styled-components": "^5.0.0-4.canary-sheet"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/node": "^7.2.2",
    "babel-loader": "^8.0.6",
    "babel-preset-setsun": "2.3.0",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "nodemon": "^1.19.0",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1"
  }
}
