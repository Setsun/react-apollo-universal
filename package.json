{
  "name": "react-apollo-universal",
  "version": "1.0.0-alpha.0",
  "description": "React Apollo Universal starter backed by Express",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-apollo-universal"
  },
  "keywords": [
    "react",
    "apollo",
    "pwa",
    "ssr"
  ],
  "dependencies": {
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.9",
    "apollo-server": "2.4.8",
    "apollo-server-express": "2.4.8",
    "express": "4.16.4",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.4",
    "history": "4.7.2",
    "http-proxy-middleware": "0.19.1",
    "react": "16.8.4",
    "react-apollo": "2.5.2",
    "react-dom": "16.8.4",
    "react-intl": "2.8.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.6.0",
    "styled-components": "^4.1.3",
    "zen-observable": "0.8.13"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/node": "7.2.2",
    "@types/express": "4.16.1",
    "@types/react": "16.8.7",
    "@types/react-dom": "16.8.2",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-setsun": "2.1.3",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "dotenv": "6.2.0",
    "eslint": "5.15.1",
    "eslint-config-eslint": "5.0.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-setsun": "2.1.3",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "ts-loader": "5.3.3",
    "typescript": "3.3.3333",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-serve": "2.0.3",
    "workbox-webpack-plugin": "4.1.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cross-env NODE_ENV=development webpack-serve --config webpack.config.js",
    "start:server": "cross-env NODE_ENV=development nodemon src/server --exec babel-node --extensions '.ts,.tsx'",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "cross-env NODE_ENV=production babel src/server.tsx --out-file dist/server.js",
    "serve": "node dist/server.js",
    "type-check": "tsc",
    "eslint-prettier-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}